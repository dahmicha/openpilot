<xml>
    <object name="PX2FlightPlan" singleinstance="true" settings="false">
        <description>State of the onboard flightplan</description>
        <field name="WaypointList1" units="pointer" type="int32" elements="1"/>
        <field name="WaypointList2" units="pointer" type="int32" elements="1"/>
        <field name="WaypointListCurrent" units="none" type="int8" elements="1"/>
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="false" updatemode="manual" period="0"/>
        <telemetryflight acked="false" updatemode="onchange" period="0"/>
        <logging updatemode="periodic" period="1000"/>
    </object>
</xml>
